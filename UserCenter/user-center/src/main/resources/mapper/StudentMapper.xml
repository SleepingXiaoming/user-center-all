<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ming.usercenter.mapper.StudentMapper">

    <resultMap id="BaseResultMap" type="com.ming.usercenter.pojo.Student">
            <id property="studentNumber" column="studentNumber" jdbcType="VARCHAR"/>
            <result property="username" column="username" jdbcType="VARCHAR"/>
            <result property="departments" column="departments" jdbcType="VARCHAR"/>
            <result property="grade" column="grade" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        studentNumber,username,departments,
        grade
    </sql>
</mapper>
